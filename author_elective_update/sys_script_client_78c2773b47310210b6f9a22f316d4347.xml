    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Use the getEmailAddress script inlclude to populate email address from Requested for field when creating new record.</description>
        <field/>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>NeedIt Populate Email field for new</name>
        <order/>
        <script><![CDATA[function onLoad() {
    //Type appropriate comment here, and begin script below
    if (g_form.isNewRecord()) {
        var getEmailAddress = new GlideAjax('GetEmailAddress');
        getEmailAddress.addParam('sysparm_name', 'getEmail');
        getEmailAddress.addParam('sysparm_userID', g_form.getValue('u_requested_for'));
        getEmailAddress.getXML(populateEmailField);
    }

    function populateEmailField(response) {
        var emailAddress = response.responseXML.documentElement.getAttribute('answer');
		g_form.setValue('u_requested_for_email', emailAddress);
		
    }
}]]></script>
